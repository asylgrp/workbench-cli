#!/usr/bin/env php
<?php

namespace asylgrp\workbench\Console;

require __DIR__.'/../vendor/autoload.php';

use Symfony\Component\Console\Application;

$app = new Application('workbench', '$app_version$');

$app->addCommands([
    new BanCommand,
    new BookCommand,
    new ImportCommand,
    new InitCommand,
    new StoreCommand,
    new SyncCommand,
]);

return $app->run();
