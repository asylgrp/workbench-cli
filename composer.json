{
    "name": "asylgrp/workbench-cli",
    "description": "CLI frontend for administration of a workbench server",
    "homepage": "https://github.com/asylgrp/workbench-cli",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Hannes Forsg√•rd",
            "email": "hannes.forsgard@fripost.org"
        }
    ],
    "bin": [
        "bin/workbench"
    ],
    "autoload": {
        "psr-4": {
            "asylgrp\\workbench\\": "src/"
        },
        "files": [
            "src/accounting_macros.php"
        ]
    },
    "require": {
        "php": "^7.1",
        "ext-bcmath": "*",
        "asylgrp/economix": "^1.1",
        "byrokrat/amount": "^2.0",
        "byrokrat/accounting": "^1.0",
        "hanneskod/yaysondb": "^2.0",
        "symfony/console": "^4",
        "symfony/dependency-injection": "^4",
        "symfony/event-dispatcher": "^4",
        "league/tactician": "^1"
    },
    "require-dev": {
        "symfony/config": "^4",
        "symfony/yaml": "^4",
        "symfony/expression-language": "^4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/asylgrp/economix"
        }
    ]
}
